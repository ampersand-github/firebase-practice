{
  "hosting": {
    "public": "out",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "/sign-in",
        "destination": "/sign-in.html"
      },
      {
        "source": "/sign-up",
        "destination": "/sign-up.html"
      }
    ]
  }
}
